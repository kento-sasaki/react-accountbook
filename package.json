{
  "name": "react-accountbook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -o build/storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "deploy": "npm run build && firebase deploy",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "eject": "react-scripts eject",
    "emu:firebase": "firebase emulators:start",
    "emu:firestore": "firebase emulators:start --only firestore",
    "emu:functions": "firebase emulators:start --only functions",
    "lint": "npm run eslint --fix ./src/**/*.tsx",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "firebase emulators:exec --only firestore 'react-scripts test'",
    "test:c": "firebase emulators:exec --only firestore 'react-scripts test --coverage --watchAll=false'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/components/**/*.{js,jsx,ts,tsx}",
      "src/**/container/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/{policy,terms}.tsx"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@google-cloud/vision": "^2.1.2",
    "@nivo/bar": "^0.62.0",
    "@nivo/pie": "^0.62.0",
    "@reduxjs/toolkit": "^1.3.5",
    "@types/lodash": "^4.14.159",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/recharts": "^1.8.10",
    "@types/storybook__addon-links": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "axios": "^0.21.1",
    "dayjs": "^1.8.28",
    "firebase": "^7.14.2",
    "lodash": "^4.17.21",
    "normalizr": "^3.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^25.2.1",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cypress": "^4.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prefer-arrow": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "fs": "^0.0.1-security",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-dom": "^4.0.0",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "uuid": "^8.1.0"
  }
}
